                         "drizzle-orm":  "^0.33.0",
                         "fastify":  "^4.27.2",
                         "pino":  "^9.4.0"
                     },
    "devDependencies":  {
                            "drizzle-kit":  "^0.26.2",
                            "prisma":  "^6.18.0"
                        }
}
